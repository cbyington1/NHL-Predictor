{
    "$schema": "https://railway.app/railway.schema.json",
    "build": {
      "builder": "NIXPACKS",
      "buildCommand": "npx expo export:web"
    },
    "deploy": {
      "numReplicas": 1,
      "startCommand": "npx serve dist",
      "healthcheckPath": "/",
      "healthcheckTimeout": 10,
      "restartPolicyType": "ON_FAILURE"
    }
  }